@model Dictionary<string,int>;
 <!-- Counts Section -->
<section id="counts" class="section counts light-background">

    <div class="container" data-aos="fade-up" data-aos-delay="100">

        <div class="row gy-4">

            <div class="col-lg-3 col-md-6">
                <div class="stats-item text-center w-100 h-100">
                    <span data-purecounter-start="0" data-purecounter-end="@Model["students"].ToString()" data-purecounter-duration="1" class="purecounter"></span>
                    <p>Зарегистрированных пользователей</p>
                </div>
            </div><!-- End Stats Item -->

            <div class="col-lg-3 col-md-6">
                <div class="stats-item text-center w-100 h-100">
                    <span data-purecounter-start="0" data-purecounter-end="@Model["courses"].ToString()" data-purecounter-duration="1" class="purecounter"></span>
                    <p>Сыграно партий</p>
                </div>
            </div><!-- End Stats Item -->

            <div class="col-lg-3 col-md-6">
                <div class="stats-item text-center w-100 h-100">
                    <span data-purecounter-start="0" data-purecounter-end="@Model["trainers"].ToString()" data-purecounter-duration="1" class="purecounter"></span>
                    <p>Сыграно турниров</p>
                </div>
            </div><!-- End Stats Item -->

            <div class="col-lg-3 col-md-6">
                <div class="stats-item text-center w-100 h-100">
                    <span data-purecounter-start="0" data-purecounter-end="@Model["webinars"].ToString()" data-purecounter-duration="1" class="purecounter"></span>
                    <p>Создано клубов</p>
                </div>
            </div><!-- End Stats Item -->

        </div>

    </div>

</section><!-- /Counts Section -->

  

   
