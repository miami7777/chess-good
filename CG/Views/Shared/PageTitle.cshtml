<!-- Page Title -->
<div class="page-title aos-init aos-animate" data-aos="fade">
    <div class="heading">
        <div class="container">
            <div class="row d-flex justify-content-center text-center">
                <div class="col-lg-8">
                    <h1>@ViewBag.Title</h1>
                    <p class="mb-0">@ViewBag.Description</p>
                </div>
            </div>
        </div>
    </div>
    <nav class="breadcrumbs">
        <div class="container">
            <ol>
                <li><a href="/">Главная</a></li>
                <li class="current">@ViewBag.Title</li>
            </ol>
        </div>
    </nav>
</div><!-- End Page Title -->

   
